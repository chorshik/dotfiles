! colors
URxvt.keysym.Mod4-F12: perl:solarized:toggle
URxvt.solarized.default: dark
!urxvt
URxvt*buffered: true
URxvt*cursorBlink: true
URxvt*underlineColor: yellow
URxvt*depth: 32
URxvt*borderless: 1
URxvt*scrollBar: false
URxvt*loginShell: true
Urxvt*secondaryScroll:  true    # Enable Shift-PageUp/Down in screen
URxvt*saveLines: 5000
URxvt*termName: rxvt-unicode
URxvt.urlLauncher: firefox
URxvt.matcher.button: 1
! font
URxvt*font: xft:Hack:size=11:antialias=true:hinting=true,xft:DejaVu Sans Mono Nerd Font Mono:size=12,xft:Terminess Powerline:size=12, xft:Twitter Color Emoji:size=9
! extencion
URxvt.perl-lib: $HOME/.urxvt/ext/
URxvt.perl-ext-common: default,solarized,clipboard
! === clipboard
!
URxvt.keysym.Control-Insert: perl:clipboard:copy
URxvt.keysym.Shift-Insert: perl:clipboard:paste

!URxvt.copyCommand:  xclip -i -selection clipboard
!URxvt.pasteCommand: xclip -o -selection clipboard

URxvt.copyCommand:  xsel -ib
URxvt.pasteCommand: xsel -ob
! ====
